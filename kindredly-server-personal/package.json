{
  "name": "tset-server",
  "version": "1.0.30",
  "description": "Kindredly API Server",
  "author": "",
  "license": "CC BY-NC 4.0 License",
  "scripts": {
    "start-task_runner": "npm run build && cross-env NODE_ENV=production node dist/task_runner.js",
    "start-task_runner-dev": "cross-env NODE_ENV=development nodemon --config nodemon-task.json",
    "start": "npm run build && cross-env NODE_ENV=production node dist/server.js",
    "start:dev": "npm run build && cross-env NODE_ENV=development node --require dist/instrumentation.js dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "swc src --out-dir dist --source-maps --copy-files --strip-leading-paths && cp .env ./dist/.env ",
    "build:tsc": "tsc && tsc-alias",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev",
    "db:dropTables": "npm run build && cross-env NODE_ENV=development   node dist/admin_cli.js --drop_all_tables",
    "db:seed": "npm run build && cross-env NODE_ENV=development    node dist/admin_cli.js --seed_dev",
    "db:refreshDev": "npm run db:dropTables && npm run migrate && npm run db:seed",
    "db:create": "npm run build && node dist/db/createdb.util.js",
    "migrate": "cross-env NODE_ENV=development knex migrate:latest --knexfile knex/knexfile.js",
    "rollback": "cross-env NODE_ENV=development knex migrate:rollback --knexfile knex/knexfile.js",
    "migrateprod": "cross-env NODE_ENV=production  knex migrate:latest --knexfile knex/knexfile.js",
    "rollbackprod": "cross-env NODE_ENV=production knex migrate:rollback --knexfile knex/knexfile.js"
  },
  "dependencies": {
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.4",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.4.1",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "envalid": "^8.0.0",
    "express": "^4.18.3",
    "express-slow-down": "^2.0.3",
    "express-validator": "^7.2.1",
    "hpp": "^0.2.3",
    "inversify": "^6.2.2",
    "ioredis": "^5.5.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "knex": "^3.1.0",
    "knexfile": "file:./knex",
    "pg": "^8.11.3",
    "rate-limiter-flexible": "^5.0.5",
    "search-index": "^4.0.0",
    "sharp-ico": "^0.1.5",
    "swc": "^1.0.11",
    "tset-sharedlib": "file:../tset-sharedlib",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.4",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.25",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "node-config": "^0.0.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
